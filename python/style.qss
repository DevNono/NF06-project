QWidget {
    font-family: Poppins;
}

QLabel {
    font-family: Poppins;
    font-weight: 600;
}

QPushButton {
    background-color: none;
    border: none;
    width: 50px;
}

QTableWidget {
    background-color: #ECECEC;
    border-radius: 12px;
} 

QTableWidget::item:selected {
    background-color: #393a3f; 
    color: #ffffff;
}